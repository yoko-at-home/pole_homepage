    "env": "vc env pull .env.local",
    "prepare": "husky install"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
