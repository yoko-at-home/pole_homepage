{
  "name": "nexst-tailwind",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "serve": "run-s build start",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "unit": "jest",
    "fix": "run-s -l -c fix:lint fix:prettier",
